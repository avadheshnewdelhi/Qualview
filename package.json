{
    "name": "qualview",
    "version": "1.0.0",
    "description": "AI Research Copilot for Figma & FigJam",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:main\" \"npm run dev:ui\"",
        "dev:main": "esbuild src/main/code.ts --bundle --outfile=dist/code.js --watch",
        "dev:ui": "vite",
        "build": "npm run build:main && npm run build:ui",
        "build:main": "esbuild src/main/code.ts --bundle --outfile=dist/code.js --minify",
        "build:ui": "vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
    },
    "dependencies": {
        "@radix-ui/react-accordion": "^1.2.2",
        "@radix-ui/react-dialog": "^1.1.4",
        "@radix-ui/react-progress": "^1.1.1",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-select": "^2.1.4",
        "@radix-ui/react-separator": "^1.1.1",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-tabs": "^1.1.2",
        "@radix-ui/react-tooltip": "^1.1.6",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.469.0",
        "mammoth": "^1.8.0",
        "openai": "^4.73.0",
        "papaparse": "^5.4.1",
        "pdfjs-dist": "^4.8.69",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "recharts": "^3.7.0",
        "tailwind-merge": "^2.5.5",
        "tesseract.js": "^5.1.1",
        "uuid": "^11.0.3",
        "xlsx": "^0.18.5",
        "zustand": "^5.0.0"
    },
    "devDependencies": {
        "@figma/plugin-typings": "^1.104.0",
        "@types/node": "^22.10.2",
        "@types/papaparse": "^5.3.15",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/uuid": "^10.0.0",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.0",
        "esbuild": "^0.24.0",
        "eslint": "^9.16.0",
        "postcss": "^8.4.49",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.7.2",
        "vite": "^6.0.3",
        "vite-plugin-singlefile": "^2.3.0"
    }
}
